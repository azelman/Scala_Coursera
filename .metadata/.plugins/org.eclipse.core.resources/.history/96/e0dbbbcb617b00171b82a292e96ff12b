object week6 {
  println("Welcome to the Scala worksheet")
  val n = 7
  val lst = 1 until n
  val lst2 = lst flatMap (i => (1 until i) map (j => (j,i)))
  def isPrime(in: (Int, Int)): Boolean = 2 until (in._1 + in._2) exists (i => in._1 + in._2 % i != 0)
  
  (lst2 filter isPrime) map ((i: Int, j: Int) => i + j)
}