object week62 {
  
  var map = "aaabbc" groupBy (x => x)             //> map  : scala.collection.immutable.Map[Char,String] = Map(b -> bb, a -> aaa, c
                                                  //|  -> c)
	(for ((x,y) <- map) yield (x, y.length)).toList //> res0: scala.collection.immutable.Map[Char,Int] = Map(b -> 2, a -> 3, c -> 1)).toList
                                                  //| 
}